{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "anhskohbo/no-captcha": "^3.3",
        "aws/aws-sdk-php": "^3.176",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "doctrine/dbal": "^3.1",
        "eluceo/ical": "^2.1",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^5.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "html2text/html2text": "^4.3",
        "jamesmills/laravel-timezone": "^1.9",
        "laravel-notification-channels/microsoft-teams": "^1.1",
        "laravel/cashier": "^13.1",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^2.2",
        "laravel/sanctum": "^2.6",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "~1.0",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "pda/pheanstalk": "^4.0",
        "pion/laravel-chunk-upload": "^1.4",
        "pragmarx/countries": "^0.7.2",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "sabberworm/php-css-parser": "^8.3",
        "spatie/calendar-links": "^1.6",
        "spatie/laravel-backup": "^6.16",
        "sssurii/laravel-ics": "^0.2.0",
        "vimeo/laravel": "^5.6",
        "vimeo/vimeo-api": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "matt-allan/laravel-code-style": "^0.6.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Utils/helpers.php",
            "app/Utils/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-style": "php-cs-fixer fix --dry-run --diff",
        "fix-style": "php-cs-fixer fix"
    }
}
